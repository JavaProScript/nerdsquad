<table>
  <thead>
    <tr>
      <th><h2>Games</h2></th>
      <th><h2>Gamerooms</h2></th>
      <th colspan="2"></th>
    </tr>
  </thead>

  <tbody>
    <% @gamerooms.each do |gameroom| %>          
